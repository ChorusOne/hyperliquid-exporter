# Sample environment variables for the monitoring script

# Set the home directory for the node (default is your $HOME/hl, e.g. /home/ubuntu/hl or /root/hl)
NODE_HOME=

# Set the binary location (default is $HOME/hl-node)
NODE_BINARY=

# Set to 'true' if this node is a validator, 'false' otherwise
IS_VALIDATOR=

# Validator address (required if IS_VALIDATOR is true)
VALIDATOR_ADDRESS=
